include_rules

INCLUDE_DIRS += -I$(MATRIX_DIR) -I$(TRACK_DIR) -I$(CLIENT_DIR) -I$(NEURAL_NETWORK_DIR) -I$(GENETIC_LEARNING_DIR)
LD_FLAGS += -lboost_program_options

include $(COMPILE_TUP)

: foreach *.cpp |> !cxx |>

include $(LINK_TUP)

: *.o $(NEURAL_NETWORK_LIB) $(CLIENT_LIB) $(TRACK_LIB) $(MATRIX_LIB) |> !linker |> neural_client

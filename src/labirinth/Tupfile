include_rules

INCLUDE_DIRS += -I$(MATRIX_DIR)
LD_FLAGS += -lboost_program_options

include $(COMPILE_TUP)

: foreach *.cpp |> !cxx |>

include $(LINK_TUP)

: main.o $(MATRIX_LIB) |> !linker |> labirinth
: generator.o $(MATRIX_LIB) |> !linker |> generator
: print.o $(MATRIX_LIB) |> !linker |> print
